{
    "name": "Jenkins Extension",
    "manifest_version": 2,
    "version": "0.9.4",
    "description": "This Extensions monitors Jenkins.",
    "icons": { "128": "images/headshot.png" },
    "options_page": "options.html",
    "background": {
      "page": "background.html"
    },
    "browser_action": {
       "default_icon": "images/headshot.png",
       "default_title": "Jenkins",
       "default_popup" : "status.html"
    },
    "permissions": [
        "tabs",
        "http://*/",
        "https://*/"
    ]
}
